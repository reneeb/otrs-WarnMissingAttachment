    <ConfigItem Name="Frontend::Output::FilterElementPost###OutputFilterWarnMissingAttachment" Required="0" Valid="1">
        <Description Translatable="1">List of screens that are affected by this module.</Description>
        <Group>WarnMissingAttachment</Group>
        <SubGroup>OutputFilter</SubGroup>
        <Setting>
            <Hash>
                <Item Key="Module">Kernel::Output::HTML::FilterElementPost::WarnMissingAttachment</Item>
                <Item Key="Debug">0</Item>
                <Item Key="Templates">
                    <Hash>
                        <Item Key="AgentTicketPhone">1</Item>
                        <Item Key="AgentTicketEmail">1</Item>
                        <Item Key="AgentTicketCompose">1</Item>
                    </Hash>
                </Item>
            </Hash>
        </Setting>
    </ConfigItem>
   <ConfigItem Name="WarnMissingAttachment::BodyField" Required="1" Valid="1">
        <Description Translatable="1">Names of Body fields.</Description>
        <Group>WarnMissingAttachment</Group>
        <SubGroup>Core::WarnMissingAttachment</SubGroup>
        <Setting>
            <Hash>
                <Item Key="AgentTicketPhone">Body</Item>
                <Item Key="AgentTicketEmail">Body</Item>
                <Item Key="AgentTicketCompose">Body</Item>
            </Hash>
        </Setting>
    </ConfigItem>
   <ConfigItem Name="WarnMissingAttachment::Keywordlist" Required="1" Valid="1">
        <Description Translatable="1">Define how the keyword list is built - either only agents' language or both user language and english.</Description>
        <Group>WarnMissingAttachment</Group>
        <SubGroup>Core::WarnMissingAttachment</SubGroup>
        <Setting>
            <Option SelectedID="localized">
                <Item Key="localized" Translatable="1">localized</Item>
                <Item Key="international" Translatable="1">localized + english</Item>
            </Option>
        </Setting>
    </ConfigItem>
