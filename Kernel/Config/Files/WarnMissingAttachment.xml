<?xml version="1.0" encoding="utf-8"?>
<otrs_config version="1.0" init="Config">
    <ConfigItem Name="Frontend::Output::FilterElementPost###OutputFilterWarnMissingAttachment" Required="0" Valid="1">
        <Description Translatable="1">List of screens that are affected by this modul.</Description>
        <Group>WarnMissingAttachment</Group>
        <SubGroup>OutputFilter</SubGroup>
        <Setting>
            <Hash>
                <Item Key="Module">Kernel::Output::HTML::FilterElementPost::WarnMissingAttachment</Item>
                <Item Key="Debug">0</Item>
                <Item Key="Templates">
                    <Hash>
                        <Item Key="AgentTicketPhone">1</Item>
                        <Item Key="AgentTicketEmail">1</Item>
                        <Item Key="AgentTicketCompose">1</Item>
                    </Hash>
                </Item>
            </Hash>
        </Setting>
    </ConfigItem>
   <ConfigItem Name="WarnMissingAttachment::BodyField" Required="1" Valid="1">
        <Description Translatable="1">Names of Body fields.</Description>
        <Group>WarnMissingAttachment</Group>
        <SubGroup>Core::WarnMissingAttachment</SubGroup>
        <Setting>
            <Hash>
                <Item Key="AgentTicketPhone">Body</Item>
                <Item Key="AgentTicketEmail">Body</Item>
                <Item Key="AgentTicketCompose">Body</Item>
            </Hash>
        </Setting>
    </ConfigItem>
   <ConfigItem Name="WarnMissingAttachment::Keywordlist" Required="1" Valid="1">
        <Description Translatable="1">Define how the keywordlist is built - either only agents' language or both user language and english.</Description>
        <Group>WarnMissingAttachment</Group>
        <SubGroup>Core::WarnMissingAttachment</SubGroup>
        <Setting>
            <Option SelectedID="localized">
                <Item Key="localized">localized</Item>
                <Item Key="international" Translatable="1">localized + english</Item>
            </Option>
        </Setting>
    </ConfigItem>
   <ConfigItem Name="WarnMissingAttachment::Keywords" Required="1" Valid="1">
        <Description Translatable="1">Keywords that indicates that an attachment should be sent. It's a fallback when no localization for the agents' language can be found.</Description>
        <Group>WarnMissingAttachment</Group>
        <SubGroup>Core::Keywords</SubGroup>
        <Setting>
            <Array>
                <Item>attach</Item>
                <Item>Attachment</Item>
                <Item>attached</Item>
            </Array>
        </Setting>
    </ConfigItem>
